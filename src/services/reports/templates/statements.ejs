<!DOCTYPE html>
<html>

<head>
  <title>Reports</title>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <style>
    @media print {

      /* Define page breaks for the responsive table */
      .responsive-table {
        page-break-inside: avoid;
      }

      .responsive-table tr {
        page-break-inside: avoid;
        page-break-after: auto;
      }

    }

    /* _______________________________________________ */
    body {
      font-family: 'Open Sans';
      font-style: normal !important;
      font-variant: normal !important;
      margin-left: auto;
      margin-right: auto;
    }

    table {
      margin-top: 20px;
      border-collapse: collapse;
      width: 100%;
      border-radius: 10px !important;
      text-align: center !important;
    }

    table,
    th,
    td {
      border: 1px solid #dddddd;
      border-radius: 10px;
      padding: 15px;
      /* border-left: none;
       border-right: none; */
    }

    th {
      border: none;
    }

    td,
    th {
      border-top: 1px solid #dddddd;
      border-bottom: 1px solid #dddddd;
      text-align: center;
      padding: 8px;

    }

    .t {
      border-left: none !important;
      text-align: center;
    }


    .rounded {
      border: 1px solid #dddddd;
    }

    .left {
      text-align: left;
      border-left: none !important;
      border-right: none !important;
    }

    .icon {
      margin-bottom: -9px;
      margin-right: 18px;
    }

    .svgs {
      margin-bottom: -9px;
      margin-right: 6px;
    }

    .dates {
      float: right;
      margin-top: 20px;
      margin-right: -122px;
    }

    .head {
      font-size: 16px;
      font-weight: bold;
      float: right;
      margin: 0;
    }

    .line {
      border-bottom: 1px solid rgb(215, 212, 212);
      margin-top: 70px;
    }

    .boxline {
      border-bottom: 1px solid rgb(215, 212, 212);
      margin-left: auto;
      margin-right: auto;
      width: 95%;
    }

    .border {
      border: 1px solid rgb(215, 212, 212);
      height: auto;
      border-radius: 15px;
    }

    .import {
      width: 95%;
      margin-top: 15px;
      margin-left: auto;
      margin-right: auto;
      margin-bottom: 15px;
    }

    .values {
      float: right;
      font-size: larger;
    }
  </style>

</head>

